{
    "name": "metamath-wasm-proof-explorer",
    "version": "1.0.0",
    "description": "Metamath Proof Explorer as a single page application",
    "scripts": {
        "build": "parcel build ./index.html ./wapm_packages/metamath/metamath.wasm ./node_modules/@wasmer/wasm-terminal/lib/workers/process.worker.js ./node_modules/xterm/css/xterm.css",
        "clean": "rimraf dist",
        "copy": "cp ./set.mm ./dist/set.mm && cp ./demo0.mm ./dist/demo0.mm",
        "lint": "eslint . --ext .ts --ext .tsx",
        "run": "ts-node express.ts",
        "start": "npm run clean && npm run build && npm run copy && npm run run"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Antony74/metamath-wasm-proof-explorer.git"
    },
    "author": "Antony74",
    "license": "MIT",
    "dependencies": {
        "@wasmer/wasm-terminal": "^0.12.0",
        "@wasmer/wasm-transformer": "^0.12.0",
        "@wasmer/wasmfs": "^0.12.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@parcel/transformer-raw": "^2.0.1",
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.4",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "eslint": "^8.5.0",
        "express": "^4.17.2",
        "parcel": "^2.1.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    }
}
